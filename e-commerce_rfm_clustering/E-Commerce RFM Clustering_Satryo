t()[i]]\n","        missingness['prop'] = [df.isna().sum().tolist()[i]/df.shape[0]]\n","    else:\n","        missingness['column'] += [df.columns[i]]\n","        missingness['missing_count'] += [df.isna().sum().tolist()[i]]\n","        missingness['prop'] += [round((df.isna().sum().tolist()[i]/df.shape[0]) * 100, 2)]\n","\n","# Converting the missingness dictionary to DataFrame\n","pd.DataFrame(missingness).sort_values('prop', ascending = False)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":300},"id":"5pyvAsM8o5l7","executionInfo":{"status":"ok","timestamp":1713932230649,"user_tz":-420,"elapsed":6680,"user":{"displayName":"Satryo Sunu","userId":"09214117207830051140"}},"outputId":"2cdd57e1-2d38-4e60-8310-8216fe400196"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["          column  missing_count  prop\n","6     CustomerNo             55  0.01\n","0  TransactionNo              0  0.00\n","1           Date              0  0.00\n","2      ProductNo              0  0.00\n","3    ProductName              0  0.00\n","4          Price              0  0.00\n","5       Quantity              0  0.00\n","7        Country              0  0.00"],"text/html":["\n","  <div id=\"df-3ad946b7-99df-4e50-8c8e-a9750a2780ab\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>column</th>\n","      <th>missing_count</th>\n","      <th>prop</th>\n","    </tr>\n","  </thead>\n","  <tbody>\n","    <tr>\n","      <th>6</th>\n","      <td>CustomerNo</td>\n","      <td>55</td>\n","      <td>0.01</td>\n","    </tr>\n","    <tr>\